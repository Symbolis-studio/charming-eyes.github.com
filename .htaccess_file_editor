# invoke rewrite engine
RewriteEngine On
RewriteBase /website-downloader-online/scraping/files/live/khrystenko.5gbfree.com_webline.ua_gmail.com_a5teq5emj2q
#Redirect 400,401,403,404,500 to main website
ErrorDocument 400 http://khrystenko.5gbfree.com
ErrorDocument 401 http://khrystenko.5gbfree.com
ErrorDocument 403 http://khrystenko.5gbfree.com
ErrorDocument 404 http://khrystenko.5gbfree.com
ErrorDocument 500 http://khrystenko.5gbfree.com

# redirect all extension to html
RewriteCond %{QUERY_STRING} ^$
RewriteRule ^(.*)\.shtml$ $1.html [nc]
RewriteCond %{QUERY_STRING} ^$
RewriteRule ^(.*)\.phtml$ $1.html [nc]
RewriteCond %{QUERY_STRING} ^$
RewriteRule ^(.*)\.htm$ $1.html [nc]
RewriteCond %{QUERY_STRING} ^$
RewriteRule ^(.*)\.php$ $1.html [nc]
RewriteCond %{QUERY_STRING} ^$
RewriteRule ^(.*)\.aspx$ $1.html [nc]
RewriteCond %{QUERY_STRING} ^$
RewriteRule ^(.*)\.asp$ $1.html [nc]
RewriteCond %{QUERY_STRING} ^$
RewriteRule ^(.*)\.jsp$ $1.html [nc]

# Add a trailing slash (/) to a URL, if the URL did not have a trailing slash before
RewriteRule ^([^.]*[^/])$ /$1/ [L,R=301]

RewriteRule ^index.html$ /website-downloader-online/scraping/files/live/khrystenko.5gbfree.com_webline.ua_gmail.com_a5teq5emj2q/ [L,R=301]
